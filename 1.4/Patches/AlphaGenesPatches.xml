<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- ========== SharpClaws ========== -->

<Operation Class="PatchOperationFindMod">
  <mods>
    <li>Alpha Genes</li>
  </mods>
	<match Class="PatchOperationSequence">
	  <operations>
		<li Class="PatchOperationReplace">
			<xpath>Defs/FactionDef[defName="TribeCivilMammals"]/xenotypeSet/xenotypeChances/Cloudpuff</xpath>
			<value>
				
			<Cloudpuff>0.20</Cloudpuff>
			<Devilpuff>0.05</Devilpuff>
			</value>
		</li>
	  </operations>
	</match>
</Operation>
</Patch>